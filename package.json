{
  "name": "cstyle-lang-server",
  "displayName": "cstyle-lang-server",
  "description": "Language support for C like languages.",
  "publisher": "robert",
  "repository": {
    "type": "git",
    "url": "https://github.com/RobertJN64/C-Style-Lang-Server"
  },
  "license": "MIT",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "cstyle",
        "aliases": [
          "cstyle"
        ],
        "extensions": [
          ".cstyle"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "cstyle",
        "scopeName": "source.cstyle",
        "path": "./syntaxes/cstyle.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^24.0.10",
    "@types/vscode": "^1.101.0",
    "@vscode/vsce": "^3.2.1",
    "typescript": "5.3",
    "vscode-languageclient": "^8.0.0"
  }
}